{
    "name": "dft/silverstripe-frontend-multiselectfield",
    "description": "Add a Silverstripe 'ListBoxField' style field to the frontend forms in your site",
      "type": "silverstripe-vendormodule",
      "homepage": "http://github.com/Dean-Forest-Tech/silverstripe-frontend-multiselectfield",
      "keywords": [
          "silverstripe",
          "forms",
          "dropdown",
          "multi select",
          "field"
      ],
      "license": "BSD-3-Clause",
      "authors": [
          {
              "name": "Dean Forest Tech",
              "homepage": "https://dean-forest-tech.uk"
          }
      ],
      "support": {
          "issues": "http://github.com/Dean-Forest-Tech/-frontend-multiselectfield/issues"
      },
      "require": {
          "silverstripe/framework": "^4 || ^5"
      },
      "extra": {
          "expose": [
              "client/dist"
          ]
      },
      "autoload": {
          "psr-4": {
              "DFT\\SilverStripe\\Gallery\\": "src/"
          }
      }
  }
